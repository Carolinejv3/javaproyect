	<queryString>
		<![CDATA[SELECT
     detalle_ventas.`Codigo` AS detalle_ventas_Codigo,
     detalle_ventas.`nombre` AS detalle_ventas_nombre,
     detalle_ventas.`precio` AS detalle_ventas_precio,
     detalle_ventas.`cantidad` AS detalle_ventas_cantidad,
     detalle_ventas.`monto_pro` AS detalle_ventas_monto_pro,
     ventas.`ID_ventas` AS ventas_ID_ventas,
     ventas.`total` AS ventas_total,
     ventas.`facturado_por` AS ventas_facturado_por,
     ventas.`fecha_venta` AS ventas_fecha_venta
FROM
     `detalle_ventas` detalle_ventas,
     `ventas` ventas

		where ventas.ID_ventas = $P{factu} and detalle_ventas.ID_ventas = $P{factu}]]>
	</queryString>
